<?xml version="1.0" encoding="utf-8"?>
<selector android:variablePadding="false"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/fbui_facebook_blue_f0" />
            <corners android:radius="@dimen/fbui_button_corner_radius" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/fbui_facebook_blue" />
            <corners android:radius="@dimen/fbui_button_corner_radius" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/fbui_blue_60" />
            <corners android:radius="@dimen/fbui_button_corner_radius" />
        </shape>
    </item>
</selector>