<?xml version="1.0" encoding="utf-8"?>
<selector android:variablePadding="false"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/video_download_background" />
            <stroke android:width="2.0dip" android:color="@color/fbui_white" />
            <corners android:radius="@dimen/overlaying_save_button_radius" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/video_download_background" />
            <stroke android:width="2.0dip" android:color="@color/fbui_bluegrey_10" />
            <corners android:radius="@dimen/overlaying_save_button_radius" />
        </shape>
    </item>
</selector>